{
	"extends": [
		":dependencyDashboard",
		":semanticPrefixFixDepsChoreOthers",
		":ignoreModulesAndTests",
		"workarounds:all"
	],

	"prHourlyLimit": 24,
	"prConcurrentLimit": 5,
	"gitIgnoredAuthors": [
		"github-actions@github.com"
	],

	"packageRules": [
		{
			"packagePatterns": [
				"*"
			],
			"enabled": false
		},
		{
			"matchFileNames": [
				"packages/minifiers/**"
			],
			"groupName": "minifiers",
			"matchPackageNames": [
				"@minify-js/node",
				"@swc/core",
				"@tdewolff/minify",
				"babel-minify",
				"bun",
				"esbuild",
				"google-closure-compiler",
				"oxc-minify",
				"terser",
				"uglify-js",
				"tedivm/jshrink"
			],
			"rangeStrategy": "bump",
			"enabled": true
		}
	]
}
